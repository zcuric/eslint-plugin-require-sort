{
  "publishConfig": {
    "access": "public"
  },
  "name": "eslint-plugin-require-sort",
  "version": "1.3.0",
  "description": "ESlint plugin for sorting requires (CommonJS modules) alphabetically",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "prepare": "npm run build",
    "build": "babel index.js -d dist",
    "release": "npx np"
